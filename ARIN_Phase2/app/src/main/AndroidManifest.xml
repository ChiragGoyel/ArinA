<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rcjoshi.arinphase2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Destination"
            android:label="@string/title_activity_destination" />
        <activity
            android:name=".SourceIdentification"
            android:label="@string/title_activity_source_identification" />
        <activity
            android:name=".NavigateBasic"
            android:label="@string/title_activity_navigate_basic"></activity>
    </application>

</manifest>